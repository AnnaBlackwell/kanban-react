{
  "env": {
    "build": {
      "optional": ["optimisation", "minification"]
    },
    "presets": [
      "react",
      "es2015",
      "survivejs-kanban",
      "react-hmre"
    ],
    "plugins": [
      "transform-runtime",
      "transform-object-rest-spread",
      "transform-class-properties",
      "lodash",
      "espower"
    ]
  }
}
